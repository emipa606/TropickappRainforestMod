        <CK_PlantTreeStranglerFig>0.0013</CK_PlantTreeStranglerFig>
        <CK_PlantWildRice>0.0003</CK_PlantWildRice>
        <CK_PlantWildTaro>0.003</CK_PlantWildTaro>
        <CK_PlantTreeWildBanana>0.001</CK_PlantTreeWildBanana>
