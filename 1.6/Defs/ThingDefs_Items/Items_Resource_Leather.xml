<Defs>
  <ThingDef ParentName="LeatherBase">
    <defName>CK_LeatherPanda</defName>
    <label>pandaskin</label>
    <description>The strong furry leather of a giant panda. Compared to most leather, it is quite a bit tougher and a bit better at all kinds of temperature regulation.</description>
    <graphicData>
      <color>(245,245,220)</color>
    </graphicData>
    <statBases>
      <MarketValue>3.4</MarketValue>
      <StuffPower_Armor_Sharp>1.12</StuffPower_Armor_Sharp>
      <StuffPower_Insulation_Cold>20</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>20</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <color>(245,245,220)</color>
      <commonality>0.01</commonality>
    </stuffProps>
  </ThingDef>
