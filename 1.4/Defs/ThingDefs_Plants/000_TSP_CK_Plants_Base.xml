			<li>Ground</li>
		</sowTags>
		<!-- <leaflessGraphicPath>Things/Plant/TreeOak_Leafless</leaflessGraphicPath> -->
		<wildBiomes></wildBiomes>
	</plant>
	<thingCategories Inherit="False">
		<li>Plants</li>
	</thingCategories>
  </ThingDef>
  
