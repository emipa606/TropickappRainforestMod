			<success MayRequire="mlie.tropickapprainforestmod">Always</success>
			<value MayRequire="mlie.tropickapprainforestmod">
				<CK_PlantFern>4</CK_PlantFern>
				<CK_PlantTreePalm>0.21</CK_PlantTreePalm>
				<CK_PlantBushPalm>0.0001</CK_PlantBushPalm>
				<CK_PlantTreeBamboo>0.005</CK_PlantTreeBamboo>
				<CK_PlantBromeliad>0.024</CK_PlantBromeliad>
				<CK_PlantTreeWildCoconutPalm>0.0025</CK_PlantTreeWildCoconutPalm>
				<CK_PlantTreeMahogany>0.0001</CK_PlantTreeMahogany>
				<CK_PlantTreeStranglerFig>0.013</CK_PlantTreeStranglerFig>
				<CK_PlantWildRice>0.0003</CK_PlantWildRice>
				<CK_PlantWildTaro>0.003</CK_PlantWildTaro>
				<CK_PlantTreeWildBanana>0.0001</CK_PlantTreeWildBanana>
				<CK_PlantWildLemongrass>0.0002</CK_PlantWildLemongrass>
				<CK_PlantHeliconia>0.15</CK_PlantHeliconia>
