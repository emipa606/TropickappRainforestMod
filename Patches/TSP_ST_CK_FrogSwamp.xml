				<CK_PlantWildBushDreamVine>0.002</CK_PlantWildBushDreamVine>
				<CK_PlantWildCotton>0.002</CK_PlantWildCotton>
				<CK_PlantRafflesia>0.002</CK_PlantRafflesia>
				<CK_SpecialTreeHive>0.0065</CK_SpecialTreeHive>
